{
  "appId": "com.bucketdev.loteriamexicana",
  "appName": "Loteria Mexicana",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "CapacitorFirebaseAuth": {
      "providers": ["google.com", "facebook.com"],
      "languageCode": "en",
      "nativeAuth": false,
      "permissions": {
        "google": ["profile", "email"],
        "facebook": ["email", "public_profile"]
      }
    }
  }
}
